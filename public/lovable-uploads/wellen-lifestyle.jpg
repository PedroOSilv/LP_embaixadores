  <circle cx="550" cy="50" r="4" fill="url(#goldGradient)" opacity="0.7"/>
  <circle cx="50" cy="350" r="4" fill="url(#goldGradient)" opacity="0.7"/>
  <circle cx="550" cy="350" r="4" fill="url(#goldGradient)" opacity="0.7"/>
